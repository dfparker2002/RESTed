<?xml version="1.0"?>
<project name="${project.name}" default="compile" basedir=".">
  <property file="build.properties"/>
  <target name="all" depends="compile">
	</target>
  <target name="compile">
    <exec executable="sh">
      <arg value="-c"/>
      <arg value="mvn -Dmaven.test.skip=true package"/>
    </exec>
  </target>
  <target name="install">
    <exec executable="sh">
      <arg value="-c"/>
      <arg value="mvn -Dmaven.test.skip=true package sling:install"/>
    </exec>
  </target>
  <target name="deploy">
    <exec executable="sh">
      <arg value="-c"/>
      <arg value="lf libs | doit cq_put"/>
    </exec>
  </target>
  <target name="clear">
    <exec executable="sh">
      <arg value="-c"/>
      <arg value="mvn -Dmaven.test.skip=true clean"/>
    </exec>
  </target>
</project>
